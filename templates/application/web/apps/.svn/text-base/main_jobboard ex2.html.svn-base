                <div class="job-list searchingstatus">
                   
					
					
					     {section name=i loop=$load}
						  {if $load[i].hari > 0}
						  <div class="rowjob ">
						
                        <div class="boximg">
						{if $load[i].img_avatar != ''}
                            <a href="{$basedomain}jobboard/detail_jobboard?id={$load[i].id_job}" class="thumbbox"><img src="{$basedomain}public_assets/personal/{$load[i].img_avatar}" height="200px" width="200px"/></a>
						{else}
								<a href="{$basedomain}jobboard/detail_jobboard?id={$load[i].id_job}" class="thumbbox"><img src="{$basedomain}public_assets/postjob/images.jpg" /></a>
						{/if}
                            <div class="caption">
                                <i>No. of shares :{$load[i].share_count} </i>
                                <i>No. of likes: {$load[i].love_count}</i>
                            </div>    
                        </div><!-- end .boximg -->    
                        <div class="entry">
                            <h3><a href="{$basedomain}jobboard/detail_jobboard?id={$load[i].id_job}">{$load[i].job_title}</a></h3>
