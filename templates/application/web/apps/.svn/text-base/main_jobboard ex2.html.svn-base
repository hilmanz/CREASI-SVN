                                <a href="#" class="company-name"><i class="icon-suitcase">&nbsp;</i>{$load[i].nama_perusahaan}</a>
								{if $load[i].status_jobs == 1}
                                <a href="#" class="company-status"><i class="icon-check-circle">&nbsp;</i> Verified</a>
								{/if}
								
                                <a href="#" class="location-name"/><i class="icon-map-marker">&nbsp;</i> {$load[i].provinsi}, {$load[i].city}</a>
								{*}<a href="{$basedomain}share/twitterShare/jobs/{$load[i].id}/jobboard" class="location-name"/><i class="glyphicon icon-log-out">&nbsp;</i> twitter share</a>{/*}
                            </div>
                            <p align='justify'>{$load[i].deskripsi|substr:0:150}</p>
                        </div><!-- end .entry -->  
                        <div class="jobInfo">
						{if $load[i].n_status_interview == 0 && $load[i].n_status == 1 }
                            <a class="label bggreen" href="{$basedomain}jobboard/detail_jobboard?id={$load[i].id_job}"> OPEN</a>
						{/if}
						{if $load[i].n_status_interview == 1 && $load[i].n_status == 1 }
							<a class="label bgyellow" href="{$basedomain}jobboard/detail_jobboard?id={$load[i].id_job}"> INTERVIEW </a>
						{/if}
						
                            <div class="countdown-job">
