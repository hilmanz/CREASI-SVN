	var id = $('.provincy option:selected').val();
	console.log(id);
	$.ajax ({
				type	 : 'POST', 
				url	 :  basedomain+'personal/ajaxcity' , 
				data:{id:id},
				dataType:'json',
				success	: function (result) 
