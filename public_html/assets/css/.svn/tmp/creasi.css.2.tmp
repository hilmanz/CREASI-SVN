@import url(http://fonts.googleapis.com/css?family=Lato:300,400,700,900,400italic);
@import url("../fonts/eufont.css");
@font-face {
    font-family: 'caviar_dreamsbold';
    src: url('../fonts/Caviar_Dreams_Bold-webfont.eot');
    src: url('../fonts/Caviar_Dreams_Bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/Caviar_Dreams_Bold-webfont.woff') format('woff'),
         url('../fonts/Caviar_Dreams_Bold-webfont.ttf') format('truetype'),
         url('../fonts/Caviar_Dreams_Bold-webfont.svg#caviar_dreamsbold') format('svg');
    font-weight: normal;
    font-style: normal;

}
@font-face {
    font-family: 'caviar_dreamsregular';
    src: url('../fonts/CaviarDreams-webfont.eot');
    src: url('../fonts/CaviarDreams-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/CaviarDreams-webfont.woff') format('woff'),
         url('../fonts/CaviarDreams-webfont.ttf') format('truetype'),
         url('../fonts/CaviarDreams-webfont.svg#caviar_dreamsregular') format('svg');
    font-weight: normal;
    font-style: normal;

}
@import url("flexslider.css");
@import url("animate.css");

html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, font, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td {
	outline: none !important;
	font-family: 'Lato',arial, sans-serif; /* 400, 700 */
}
html{
	background:#fbfbfb;
}
img {
	border:none 0;
	outline:none;
}
input {outline:none !important;}
iframe {border:0;}
.clear {
	clear:both;
	height:0;
	font-size:0;
}
ul {
	padding:0;
	margin:0;
}
li {
	padding:0;
	margin:0;
	list-style:none;
}
.fl {float:left;}
.fr {float:right;}
.opacity {
	opacity: 0;
	filter: alpha(opacity=0);
}
iframe {
	width:100% !important;
	margin:0 !important;
	padding:0 !important;
	border:0 !important;
	max-width:100% !important;
	overflow-y: hidden !important;
}
html { font-size: 100%;}
::selection{
	background: #780006;
	color: #FFF;
}
::-moz-selection { color: #fff;  background-color: #780006;}
::-webkit-input-placeholder {
	font-size:12px;
}
:-moz-placeholder { /* Firefox 18- */
	font-size:12px;
}
::-moz-placeholder {  /* Firefox 19+ */
	font-size:12px;
}
:-ms-input-placeholder {  
	font-size:12px;
}
a {
	color: #780006;
	text-decoration:none !important;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
a:hover { color: #c00; text-decoration:none;}
/*a:focus { color: #c00;}*/
strong { font-weight: 600;}
.center { text-align: center;}
.justify {text-align: justify;}
p {
	padding:0;
	margin:0 0 20px 0;
}
*,
*:before,
*:after {
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
		  outline:none;
}
.red{	color:#e54242; }
.green{	color:#060; }
.grey{	color:#333; }

td .red{
	background:#e54242;
	color:#fff;
	padding:0 30px 0 8px;
	line-height:20px;
	border-radius:5px;
	display:inline-block;
	font-size:12px;
	position:relative;
}
td h4.red{
	margin:0;
}
td h4.red a{
	position:absolute;
	right:0;
	top:0;
	color:#fff;
}
td .green{
	background:#060;
	color:#fff;
	padding:0 8px;
	line-height:20px;
	border-radius:5px;
	font-size:12px;
	display:inline-block;
}
td .grey{
	background:#333;
	color:#fff;
	padding:0 8px;
	line-height:20px;
	border-radius:5px;
	display:inline-block;
	font-size:12px;
}
/** ========================= MAIN ========================= **/
body {
	color: #333;
	background:#fbfbfb;
	font-family: 'Lato',arial, sans-serif; /* 400, 700 */
	font-weight:300;
	font-size: 14px;
	line-height:1.4;
	margin: 0;
	overflow-x:hidden !important;
	min-width:1200px;
}
h1,h2,h3,h4,h5{
	font-weight:900;
	line-height:1.4;
}
.wrapper{
	width:1200px;
	min-width:1200px;
	margin:0 auto;
	display:table;
}
.blockwrapper{
	max-width:900px;
	margin:0 auto;
}
.row{
	overflow:hidden;
	padding:10px 0;
	clear:both;
}
.section{
	padding:40px 0;
}
.section-bordered{
	border-top:solid 5px #f2f2f2;
	border-bottom:solid 5px #f2f2f2;
}
.entry{
	padding:20px;
}
.center{
	text-align:center;
}
.ico{
	font-size:16px;
	display:inline-block;
	line-height:40px;
	vertical-align:top;
	text-align:center;
}
.ico:before{
	display:inline-block;
	line-height:40px;
	vertical-align:top;
}

#mobnav-btn {
	display:none;
}
.mobnav-subarrow {
    display: none;
}
/** ========================= COLOM ========================= **/
.col-content{width:900px;}
.col-sidebar{width:280px; margin:0 20px 0 0;}
.col1{ width:100%;}
.col2{ width:50%;}
.col3{ width:33.333333%}
.col4{ width:25%;}
.col5{ width:20%;}
.col6{ width:16.6666667%}
.col-content,.col-sidebar,.col1,.col2,.col3,.col4,.col5,.col6{
	float:left;
	padding:0 10px;
}
/** ========================= TOP HEADER ========================= **/
#top{
	background:#e54242;
	color:#fff;
	font-size:12px;
}
#top a{
	color:#fff;
}
#top a:hover{
	background:#fff;
	color:#e54242;
}
#top i.ico{
	height:60px;
}
.topInfo .ico:before{
	line-height:60px;
	text-align:center;
	display:block;
	height:60px;
}
.topInfoleft a:hover{
	background:none !important;
	color:#fff !important;
}
.topInfo a,.topInfo span{
	display:inline-block;
	line-height:60px;
	vertical-align:top;
	padding:0 10px;
	float:left;
}
.topInfoleft span{
	float:none !important;
}
.topInfo span.divider{
	padding:0;
}
#header{
	clear:both;
	border-bottom:solid 1px #ddd;
	height:100px;
}
#header .wrapper{
	overflow:inherit;
	display:table;
}
#logo {
	max-width:400px;
	float:left;
	margin:0 0 0 10px;
}
#logo img{
	width:100%;
}
.headerboxbtn{
	position:relative;
}
.smallthumb{
	width:40px; height:40px;
	float:left;
}
.smallthumb img{
	width:100%;
}
#toggleuser{
	float:left;
	display:block;
	margin:28px 10px;
	width:80px;
	background:#3d3d3d;
	position:relative;
}
#toggleuser i{
	float:left;
	width:40px;
	color:#fff;
}
#usermenubox{
	display:none;
}
.bubble {
	position:absolute;
	z-index:10000;
	width: 160px;
	height: 200px;
	padding: 0px;
	background: #ddd;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	top: 95px;
	right: 15px;
}

.bubble:after {
	content: '';
	position: absolute;
	border-style: solid;
	border-width: 0 15px 19px;
	border-color: #ddd transparent;
	display: block;
	width: 0;
	z-index: 1;
	top: -19px;
	left: 65px;
}
#usermenubox i.ico, #usermenubox i.ico:before{
	float:left;
	line-height:30px !important;
}

#usermenubox ul{
	margin:0;
	padding:7px 15px 0 15px;
}
#usermenubox li{
	clear:both;
	overflow:hidden;
	line-height:30px;
	border-bottom:solid 1px #d2d2d2;
	color:#626262;
}
#usermenubox li:last-child{
	border:none;
}
#usermenubox li a{
	display:block;
	color:#626262;
}
#usermenubox li a:hover{
	color:#c00;
}
/** ========================= MAIN MENU ========================= **/
#nav {
	float:right;
	position:relative;
}
#nav i.ico,
#nav i.ico:before{
	display:block;
	line-height:40px;
	height:30px;
	font-size:16px;
	text-align:center;
}
#main-menu{
	margin:0 20px 0 0;
}
#main-menu a span{
	display:block;
	text-align:center;
	line-height:30px;
	height:30px;
}
#main-menu a {
	color:#626262;
	padding:20px;
	line-height:90px;
	text-decoration:none;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
	font-size:12px;
	display:inline-block;
	text-transform:uppercase;
}
#main-menu a:hover,.sfHover a.sf-with-ul{
	background:#e54242;
	color: #fff;
}
#main-menu li {
	font-size: 14px;
	background:none;
	float:left;
}
#main-menu li li {
	border:none;
	margin: 0;
	font-weight: 400;
	display:block;
	float:none;
	width:200px;
}
#main-menu li li a, #main-menu li li li a {
	margin: 0;
	padding:0 15px;
	border-top: none;
	background: #edebec;
	border-bottom:solid 1px #dcdadb;
	display:block;
	height:auto;
	line-height:35px;
}
#main-menu li li a:hover {
}
#main-menu li ul {
	z-index: 9999;
	box-shadow: 0 1px 3px 1px rgba(0, 0, 0, 0.2);
	width:200px;
	border-top:solid 5px #e54242;
}
#main-menu li li ul {
	margin-left: -1px;
}
#main-menu a.sf-with-ul {
}
#main-menu li ul li a.sf-with-ul {
	padding-right: 2.25em;
}
.sf-arrows .sf-with-ul:after{
	display:none;
}
#nav .button1{
	float:left;
	margin:28px 10px;
}
.topInfo .toggleSearch{
	padding:0 10px;
	cursor:pointer;
}
/** ========================= SEARCH FORM ========================= **/
.searchform{
	position:relative;
	background:#f0f0f0;
	padding:10px;
	border-radius: 5px;
}
.shorter .searchform{
	padding:0;
}
.shorter .searchform input[type=text]{
	height:35px;
}
.shorter .searchform input[type=submit]{
	height: 35px;
	width: 35px;
	top: 0;
	right: 0;
}
.topInfo .searchform{
	float:left;
	padding:0 8px 0 0;
	box-shadow: 0 2px 30px rgba(0,0,0,0.2);
	border-radius: 5px;
	display:none;
	margin:0 10px;
	top:15px;
}
.topInfo .searchform input[type=text]{
	border:none;
	background: #fff;
	padding: 0 10px;
	height: 30px;
	border-radius: 5px 0 0 5px;
	box-shadow:1px 1px 3px rgba(0,0,0,0.2) inset;
	width:88%;
}
.topInfo .searchform input[type=submit]{
	background:#e54242;
	color:#fff;
	position:absolute;
	border:medium;
	border-radius:0 5px 5px 0;
	height:30px;
	width:30px;
	top:auto;
	right:auto;
}
.searchform input[type=text]{
	border: solid 1px #ddd;
	background: #fff;
	padding: 0 10px;
	height: 40px;
	border-radius: 5px 0 0 5px;
	box-shadow:1px 1px 3px rgba(0,0,0,0.2) inset;
	width:88%;
}
.searchform input[type=submit]{
	background:#e54242;
	color:#fff;
	position:absolute;
	border: solid 1px #ddd;
	border-radius:0 5px 5px 0;
	height:40px;
	width:40px;
	top:10px;
	cursor:pointer;
	right:10px;
}
#nav .searchform input[type=text]{
	width:auto;
}
#nav .searchform input[type=submit]{
	top:0;
	right:0;
}
/** ========================= SLIDER ========================= **/
#homeBanner{
	background:#4d0009;
	box-shadow:0 0 100px rgba(0,0,0,0.2) inset;
}
#bannerwrapper{
	margin:0 auto;
	position:relative;
	overflow:hidden;
	text-align:center;
}
/** ========================= WELCOME TEXT ========================= **/
#welcometext{
	background:#fff;
}
#welcometext .button1,#welcometext .button2,.postJobs .button2{
	margin:0 15px;
}
.section h2{
	font-size:24px;
	margin:0 0 20px 0;
    font-family: 'caviar_dreamsregular';
	font-weight:normal;
}
.titlebox{
	max-width:800px;
	margin:0 auto;
}

/** ========================= HOW TO ========================= **/
#howto h3{
	font-size:18px;
    font-family: 'caviar_dreamsregular';
	font-weight:normal;
}
.circle,.circleicon{
	width:80px; height:80px;
	border-radius:500px;
	border:solid 1px #999;
	display:inline-block;
	vertical-align:top;
	line-height:80px;
	font-size:40px;
    font-family: 'caviar_dreamsregular';
}
.circle-red{
	background:#e54242;
	color:#fff;
	border:solid 1px #fff;
}
.circleicon{
	background:#fbfbfb;
	border:none;
	margin:0 0 20px 0;
}
.circleicon .ico{
	width:80px; height:80px;
}
.circleicon .ico:before{
	width:80px; height:80px;
	font-size:40px;
	line-height:80px;
	color:#727272;
}
/** ========================= CATEGORY BOX ========================= **/
#categorybox{
	background:#fff;
}
#categorybox .wrapper{
	max-width:800px;
	min-width:inherit;
}
.boxcat{
	display:inline-block;
	vertical-align:top;
	text-align:center;
	padding:20px;
	color:#626262;
	border-radius:300px;
}
.boxcat .ico,.boxcat .ico:before{
	display:block;
	height:55px;	
	font-size:30px;
}
.boxcat span{
	display:block;
	font-size:16px;
    font-family: 'caviar_dreamsregular';
	font-weight:normal;
}
/** ========================= FEATURE JOBS ========================= **/
#featurejobs{
	background:#fff;
}
.row-btn{
	padding:30px 0;
}
.box a:hover{
	text-decoration:underline !important;
}
#featurejobs .box{
	min-height:300px;
}
.box{
	width:23%;
	margin:1%;
	display:inline-block;
	max-width:250px;
	vertical-align:top;
	position:relative;
	padding:5px;
	border:solid 1px #ddd;
	border-bottom:solid 7px #ddd;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.thumbbox img{
	width:100%;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.thumbbox:hover{
	text-decoration:none;
}
.boximg{
	position:relative;
	background:#e54242;
}
.box:hover{
	border-bottom:solid 7px #e54242;
}
.box:hover .thumbbox img{
	opacity:0.3;
}
.captionhide{
	position:absolute;
	bottom:20px;
	width:100%;
	padding:20px 0;
	opacity:0;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.box:hover .captionhide{
	opacity:1;
}
.author-name{
	font-weight:900;
}
.author-box a,.author-box span{
	display:block;
	color:#fff;
}
.caption h4 a{
    font-family: 'caviar_dreamsregular';
	font-weight:normal;
	font-size:14px;
	color:#333;	
}
.meta-info{
	margin:20px 0;
}
.meta-info a{
	color:#fff;
	font-size:12px;
	margin:0 5px;
}
.meta-info .ico,.meta-info .ico:before{
	line-height:20px;
	color:#fff;
	width:20px;
}
.ribbon {
	background-color:#ffbc40;
	position:absolute;
	padding:2px 2px 2px 20px;
	top:5px;
	left:-10px;
	height:30px;
	width:95px;
	text-align:left;
	line-height: 30px;
	font-family: georgia;
	font-style: italic;
	font-size: 14px;
  -webkit-box-sizing: content-box;
     -moz-box-sizing: content-box;
          box-sizing: content-box;
}
.ribbon:after {
	display:block;
	position:absolute;
	content:"";
	height:0;
	width:0;
	left:115px;
	top:0;
	border-top:17px solid #ffbc40;
	border-right:14px solid transparent;
	border-bottom:17px solid #ffbc40;
	border-left:14px solid #ffbc40;
  -webkit-box-sizing: content-box;
     -moz-box-sizing: content-box;
          box-sizing: content-box;
}
.ribbon:before {
	display:block;
	position:absolute;
	content:"";
	height:0;
	width:0;
	top:34px;
	left:0px;
	border-top:10px solid #555;
	border-left:10px solid transparent;
  -webkit-box-sizing: content-box;
     -moz-box-sizing: content-box;
          box-sizing: content-box;
}
.ribbonred{
	background-color:#ec4731;
	color:#fff;
}
.ribbonred:after{
	border-top:17px solid #ec4731;
	border-right:14px solid transparent;
	border-bottom:17px solid #ec4731;
	border-left:14px solid #ec4731;
}
.ribbonred:before{
}
.rowjob{
	overflow:hidden;
	border:solid 1px #f4f4f4;
	border-bottom:solid 5px #696969;
	background:#fff;
	position:relative;
	margin:0 0 20px 0;
}
.rowjob:hover{
	border-bottom:solid 5px #C00;
}
.rowjob .boximg{
	width:25%;
	float:left;
	background:none;
	padding:10px 0 0 10px;
}
.rowjob .entry{
	width:50%;
	float:left;
	padding:0 20px;
}
.rowjob .entry h3{
	font-weight: 400;
	margin: 5px 0;
	font-size: 22px;
}
.rowjob .entry h3 a{
	color:#333;
}
.rowjob .caption{
	position:absolute;
	left:0;
	bottom:0;
	width:100%;
	padding:10px;
	background:#fff;
}
.rowjob .caption i{
	display:block;
	font-style:italic;
	font-size:12px;
}
h4.draft{
	margin:0;
	padding:70px 0 10px 0;
}
.redbox-entry{
	padding:40px;
}
.errorjob{
	position:absolute;
	width:100%;
	left:0; top:0;
	background:#c00;
	background:rgba(204,0,0,0.8);
	color:#fff;
	text-align:center;
	height:100%;
	z-index:10;
}
.errorjob h3{
	font-weight:300;
	font-size:40px;
}
.jobInfo{
	width:25%;
	float:left;
	background:#f4f4f4;
	min-height:220px;
	text-align:center;
}
.jobInfo p{
	margin:0;
	padding:5px 0;
}
.label{
	display:inline-block;
	padding:0 15px;
	line-height:30px;
	text-align:center;
	text-transform:uppercase;
	border-radius:5px;
	margin:20px 0 10px 0;
}
.bggreen{
	background:#52a316;
}
.bgyellow{
	background:#FC0;
}
.bggrey{
	background:#888;
}
.countdown-job{
	text-align:center;
}
.countdown-job h4{
	background:#fff;
	border-radius:3px;
	border:solid 1px #ddd;
	border-bottom:solid 4px #eee;
	padding:10px 0;
	width:65px;
	display:inline-block;
	vertical-align:top;
	margin:0;
	line-height:43px;
	position:relative;
	font-size:30px;
}
.countdown-job h4:after{
	content: "";
	position: absolute;
	left: 0;
	width: 65px;
	height: 1px;
	background: #ddd;
	top: 50%;
	box-shadow: 0 0 10px rgba(0,0,0,0.6);
	z-index:1;
}
.countdown-job h4 b{
	position:relative;
	z-index:2;
}
.countdown-job span{
	width:40px;
	display:inline-block;
	font-size:12px;
	text-align:left;
	vertical-align:top;
	padding:2px 0;
	margin:0 0 0 10px;
}
.applicantsNo{
	display:block;
	margin:10px 0;
	font-size:12px;
}
.meta-post a{
	color:#666;
	font-size:12px;
	margin:0 20px 0 0;
}
.meta-post a.location-name{
	display:block;
	margin:10px 0;
}
