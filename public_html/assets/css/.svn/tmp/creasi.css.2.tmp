.left-ribbon {
	font-size: 14px;
	line-height:24px;
	background-color: #ffbc40;
	position: absolute;
	height: 24px;
	width: 81px;
	float: right;
	text-align: center;
	left: -5px;
	top: 24px;
	text-transform:uppercase;
	-webkit-transform:rotate(-45deg);
	-moz-transform:rotate(-45deg);
	-ms-transform:rotate(-45deg);
}
.left-ribbon:before {
	display:block;
	position:absolute;
	content:"";
	height:0;
	width:0;
	top:0;
	left:-23px;
	border-top:24px solid transparent;
	border-right:24px solid #ffbc40;
  -webkit-box-sizing: content-box;
     -moz-box-sizing: content-box;
          box-sizing: content-box;
}
.left-ribbon:after {
	display:block;
	position:absolute;
	content:"";
	height:0;
	width:0;
	top:0;
	left:80px;
	border-top:24px solid transparent;
	border-left:24px solid #ffbc40;
  -webkit-box-sizing: content-box;
     -moz-box-sizing: content-box;
          box-sizing: content-box;
}
